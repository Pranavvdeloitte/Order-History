<mvc:View
    xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:layout="sap.ui.layout"
    xmlns="sap.m"
    xmlns:uxap="sap.uxap"
    xmlns:f="sap.ui.layout.form"
    xmlns:core="sap.ui.core"
    xmlns:upload="sap.m.upload"
    xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
    controllerName="zorderhistory.controller.Display"
    displayBlock="true"
    class="sapUiSizeCompact"
>
    <App id="idAppDisplay">
        <pages>
            <Page
                id="idPageDisplay"
                class="sapUiResponsiveContentPadding"
                showHeader="false"
            >
                <content>
                    <Text text="{localDataModel>/Company}" />
                    <Text text="{localDataModel>/Entity}" />
                    <Text text="{localDataModel>/Country}" />
                    <Text text="{localDataModel>/Address}" />
                </content>
            </Page>
        </pages>
    </App>
</mvc:View>
